{
  "title": "Keyboard Brightness Adjustment",
  "rules": [
    {
      "description": "command-f1 and f2 to adjust keyboard brightness",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f1",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "illumination_decrement"
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "f2",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "illumination_increment"
            }
          ]
        }
      ]
    }
  ]
}
